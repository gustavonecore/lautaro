{
	"name": "gustavonecore/lautaro",
	"description": "Micro framrwork v2 based on Leftaro framework",
	"type": "library",
	"license": "MIT",
	"authors": [
		{ "name": "Gustavo Delgado R.", "email": "gustavo@onecore.cl" }
	],
	"require": {
        "php": ">=7.0",
        "psr/http-message": "~1.0",
		"nikic/fast-route": "^1.2",
		"zendframework/zend-diactoros": "^1.6",
		"hassankhan/config": "^0.10.0",
		"php-di/php-di": "^5.4",
		"monolog/monolog": "^1.23",
		"twig/twig": "^2.4",
		"gustavonecore/php-sanitizer": "^1.0@dev",
		"league/tactician": "~0.6",
		"cocur/slugify": "^3.0",
		"guzzlehttp/guzzle": "^6.3",
		"propel/propel": "^2.0@alpha",
		"robmorgan/phinx": "^0.10.7",
		"ramsey/uuid": "^3.8",
		"vlucas/phpdotenv": "^3.3",
		"symfony/console": "^4.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.3",
		"symfony/maker-bundle": "^1.11"
	},
    "autoload": {
    	"psr-4": {
     		"Leftaro\\": "src/",
            "Leftaro\\Test\\": "tests/"
    	}
    },
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	},
    "minimum-stability": "dev",
    "prefer-stable": true
}
